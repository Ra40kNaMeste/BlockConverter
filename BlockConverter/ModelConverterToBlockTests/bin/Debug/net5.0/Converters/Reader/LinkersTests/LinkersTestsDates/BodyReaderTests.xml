<?xml version="1.0" encoding="utf-8" ?>
<Dates>
	<Data Name="Context">
		<Property Name="Name"/>
    <Body Name="CycleBody" Start="{" End="}"/>
    <Body Name="Condition" Start="(" End=")"/>
  </Data>
  <Data Name="Function" Content="#(Name)" ChildBlocks="#(CycleBody)" EndBlockContent="#(Name)">
    <Realizations>
      <Realization Template="#(!w)#(Name)#(*s)#(CycleBody)"/>
    </Realizations>
  </Data>

  <Data Name="Operation" Content="#(Condition)">
		<Realizations>
			<Realization Template="#(Condition)"/>
		</Realizations>
	</Data>
</Dates>
